{
  "expo": {
    "name": "hansAppMovil",
    "slug": "hansAppMovil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/Auto.ico",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/Logo.ico",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "pothosPermission": "Allow $(PRODUCT_NAME) to access your photos",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/Auto.ico",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA_ROLL"
      ]
    },
    "web": {
      "favicon": "./images/Auto.ico"
    }
  }
}
